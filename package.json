    "ios": "react-native run-ios --scheme AMIaide",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint index.js App.tsx src --fix --ext .js,.ts,.tsx && pnpm format",
    "lint-quiet": "eslint index.js --quiet App.tsx src --fix --ext .js,.ts,.tsx && pnpm format",
    "ts": "tsc --noEmit",
    "postinstall": "jetifier && patch-package",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:bundle": "cd ./android; ./gradlew bundleRelease; cd ..",
    "assemble": "cd ./android; ./gradlew assembleRelease; cd ..",
    "deploy": " aws s3 cp build s3://admin.ami-app.com/ --recursive",
    "i18n:update_local": "i18next-scanner --config scripts/i18next-scanner.config.js",
    "i18n:update_remote": "node ./scripts/update-remote.js",
    "i18n:read_remote": "node ./scripts/read-remote.js"
